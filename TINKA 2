#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Cuantos bolillas son los maximos: ";
    cin >> n;
    for(int i =1; i<n-4; i++){
        for(int j = i+1; j<n-3; j++){
            for(int k =j+1; k<n-2; k++){
                for(int l = k+1; l< n-1; l++){
                    for(int m = l+1; m < n; m++){
                        cout << i << " " << j << " " << k << " " << l << " " << m << endl;
                     
                    }
                }
            }
        }
    }
    return 0;
}
